<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PackageId>KaiNet.Net</PackageId>
    <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    <Description>Net is an extensible networking library that is easy to get running. It is ideal for hobbiest programmers and rapid development as most of the networking features</Description>
    <RepositoryUrl>https://github.com/KaiNet-X/Network</RepositoryUrl>
    <Company>KaiNet</Company>
    <Version>2.0.0</Version>
	<GenerateDocumentationFile>true</GenerateDocumentationFile>
	<PackageProjectUrl>https://github.com/KaiNet-X/Network</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MessagePack" Version="2.3.85" />
  </ItemGroup>

</Project>
